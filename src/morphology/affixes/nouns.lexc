+Sg+PxDu2+Ins:%>%{ЫИØ%}%^PxDu2н%>тыл K ;
+Sg+PxDu3+Ins:%>%{ТØ%}%{ЭЕLong%}н%>тыл K ;
+Sg+PxPl1+Ins:%>%{УЮØ%}в%>тыл K ;
+Sg+PxPl2+Ins:%>%{ЫИØ%}%^PxPl2н%>тыл K ;
+Sg+PxPl3+Ins:%>%{АЯØ%}ныл%>тыл K ;

LEXICON SG-LAT
+Sg+Lat:%>%{ЫØ%}н K ;
+Sg+PxSg1+Lat:%>%{УЮØ%}м%>н K ;
+Sg+PxSg2+Lat:%>%{ЫИØ%}н%>н K ;
+Sg+PxSg3+Lat:%>%{ТØ%}%{ЭЕ%}%>н K ;
+Sg+PxDu1+Lat:%>ме̄н%>н K ;
+Sg+PxDu2+Lat:%>%{ЫИØ%}%^PxDu2н%>н K ;
+Sg+PxDu3+Lat:%>%{ТØ%}%{ЭЕLong%}н%>н K ;
+Sg+PxPl1+Lat:%>%{УЮØ%}в%>н K ;
+Sg+PxPl2+Lat:%>%{ЫИØ%}%^PxPl2н%>н K ;
+Sg+PxPl3+Lat:%>%{АЯØ%}ныл%>н K ;

LEXICON SG-LOC
+Sg+Loc:%>%{ЫØ%}т K ;
+Sg+PxSg1+Loc:%>%{УЮØ%}м%>%{ЫØ%}т K ;
+Sg+PxSg2+Loc:%>%{ЫИØ%}н%>%{ЫØ%}т K ;
+Sg+PxSg3+Loc:%>%{ТØ%}%{ЭЕ%}%>%{ЫØ%}т K ;
+Sg+PxDu1+Loc:%>ме̄н%>%{ЫØ%}т K ;
+Sg+PxDu2+Loc:%>%{ЫИØ%}%^PxDu2н%>%{ЫØ%}т K ;
+Sg+PxDu3+Loc:%>%{ТØ%}%{ЭЕLong%}н%>%{ЫØ%}т K ;
+Sg+PxPl1+Loc:%>%{УЮØ%}в%>%{ЫØ%}т K ;
+Sg+PxPl2+Loc:%>%{ЫИØ%}%^PxPl2н%>%{ЫØ%}т K ;
+Sg+PxPl3+Loc:%>%{АЯØ%}ныл%>%{ЫØ%}т K ;


LEXICON SG-NOM
+Sg+Nom: K ;
+Sg+PxSg1+Nom:%>%{УЮØ%}м K ;
+Sg+PxSg2+Nom:%>%{ЫИØ%}н K ;
+Sg+PxSg3+Nom:%>%{ТØ%}%{ЭЕ%} K ;
+Sg+PxDu1+Nom:%>ме̄н K ;
+Sg+PxDu2+Nom:%>%{ЫИØ%}%^PxDu2н K ;
+Sg+PxDu3+Nom:%>%{ТØ%}%{ЭЕLong%}н K ;
+Sg+PxPl1+Nom:%>%{УЮØ%}в K ;
+Sg+PxPl2+Nom:%>%{ЫИØ%}%^PxPl2н K ;
+Sg+PxPl3+Nom:%>%{АЯØ%}ныл K ;

LEXICON SG-TRA
+Sg+Tra:%>%{ЫИØ%}г K ;

LEXICON DU-ABL
