!! Noun inflection for Mansi
! ---------------

!! Stems are divided in four groups:
!! # StemC	 = ending in C
!! # stemCC = ending in CC
!! # StemV1 = ending in -a
!! # StemV2 = ending in other vowels


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
LEXICON NounNumber-StemC
 N_ ;
!+N+Sg:        NounCase-StemC ;
!+N+Du:ыг       NounCase-StemC ;
!+N+Pl:ыт       NounCase-StemC ;

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
LEXICON NounCase-StemC
 N_ ;
!+Nom:         # ;
!+Lat:н        # ;
!+Loc:т        # ;
!+Abl:ныл      # ;
!+Ins:ыл       # ;
!+Tra:ыг       # ;



LEXICON NounNumber-StemCC
 N_ ;
!+N+Sg:0        NounCase-StemCC ;
!+N+Du:ыг       NounCase-StemC  ;
!+N+Pl:ыт       NounCase-StemC  ;

LEXICON NounCase-StemCC

+Nom:0        # ;
+Lat:ын       # ;
+Loc:ыт       # ;
+Abl:ныл      # ;
+Ins:ыл       # ;
+Tra:ыг       # ;


LEXICON NounNumber-StemV1
 N_ ;
!+N+Sg:0        NounCase-StemV1 ;
!+N+Du:г        NounCase-StemC  ;
!+N+Pl:т        NounCase-StemC  ;

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
LEXICON NounCase-StemV1

+Nom:0       VPХ ;
+Lat:н       # ;
+Loc:т       # ;
+Abl:ныл     # ;
+Ins:л       # ;
+Tra:г       # ;


LEXICON NounNumber-StemV2
 N_ ;
!+N+Sg:0        NounCase-StemV2 ;
!+N+Du:йыг      NounCase-StemC  ;
!+N+Pl:т        NounCase-StemC  ;

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
LEXICON NounCase-StemV2

+Nom:0       # ;
+Lat:н       # ;
+Loc:т       # ;
+Abl:ныл     # ;
+Ins:л       # ;
+Tra:йыг     # ;



!! !!Possessive suffixes

LEXICON VPХ
 # ; ! Absolute declension.
+PxSg1:м # ;
+PxSg2:н # ;
+PxSg3:тэ # ;
+PxDu1:ме̄н # ;
+PxDu2:н # ;
+PxDu3:те̄н # ;
+PxPl1:в # ;
+PxPl2:н # ;
+PxPl3:ныл # ;






! 
! LEXICON N_РУМА !! рума:рума
! +N: N_ ;
! 
! LEXICON N_АКИ !! аки:аки
! +N: N_ ;
! 
! LEXICON N_КОЛ !! кол:кол 
! +N: N_ ;
! 
! LEXICON N_ПАЛЬ !! паль:паль 
! +N: N_ ;
! 
!! JACK TESTING 
LEXICON N_
+N: SG-ABL ;
+N: SG-INS ;
+N: SG-LAT ;
+N: SG-LOC ;
+N: SG-NOM ;
+N: SG-TRA ;

+N: DU-ABL ;
+N: DU-INS ;
+N: DU-LAT ;
+N: DU-LOC ;
+N: DU-NOM ;

+N: PL-ABL ;
+N: PL-INS ;
+N: PL-LAT ;
+N: PL-LOC ;
+N: PL-NOM ;

LEXICON SG-ABL
+Sg+Abl:ныл K ;
!!! NB: we list POSSESSIVE suffixes before CASE MARKERS -- 
!!! as they appear in surface forms in all UGRIC LANGUAGES
+Sg+PxSg1+Abl:%>%{УЮØ%}м%>ныл K ;
+Sg+PxSg2+Abl:%>%{ЫИØ%}н%>ныл K ;
+Sg+PxSg3+Abl:%>%{ТØ%}%{ЭЕ%}%>ныл K ;
+Sg+PxDu1+Abl:%>ме̄н%>ныл K ;
+Sg+PxDu2+Abl:%>%{ЫИØ%}%^PxDu2н%>ныл K ;
+Sg+PxDu3+Abl:%>%{ТØ%}%{ЭЕLong%}н%>ныл K ;
+Sg+PxPl1+Abl:%>%{УЮØ%}в%>ныл K ;
+Sg+PxPl2+Abl:%>%{ЫИØ%}%^PxPl2н%>ныл K ;
+Sg+PxPl3+Abl:%>%{АЯØ%}ныл%>ныл K ;

LEXICON SG-INS
+Sg+Ins:%>%{ТØ%}ыл K ;
+Sg+PxSg1+Ins:%>%{УЮØ%}м%>тыл K ;
+Sg+PxSg2+Ins:%>%{ЫИØ%}н%>тыл K ;
+Sg+PxSg3+Ins:%>%{ТØ%}%{ЭЕ%}%>тыл K ;
